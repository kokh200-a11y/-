   "source": [
    "\n",
    "# Пример 1: по типу ТС\n",
    "if 'vehicle_type' in df.columns and 'fare_amount' in df.columns:\n",
    "    g1 = df.groupby('vehicle_type').agg(\n",
    "        trips=('vehicle_type','count'),\n",
    "        revenue=('fare_amount','sum'),\n",
    "        avg_fare=('fare_amount','mean')\n",
    "    ).sort_values('revenue', ascending=False)\n",
    "    display(g1)\n",
    "\n",
    "# Пример 2: по городу и часу\n",
    "if {'pickup_city','pickup_hour','fare_amount'} <= set(df.columns):\n",
    "    g2 = df.groupby(['pickup_city','pickup_hour']).agg(\n",
    "        trips=('pickup_hour','count'),\n",
    "        revenue=('fare_amount','sum'),\n",
    "        avg_fare=('fare_amount','mean')\n",
    "    ).reset_index()\n",
    "    display(g2.head(10))\n",
    "\n",
    "# Пример 3: по статусу\n",
    "if 'status' in df.columns and 'fare_amount' in df.columns:\n",
    "    g3 = df.groupby('status').agg(\n",
    "        trips=('status','count'),\n",
    "        revenue=('fare_amount','sum'),\n",
    "        avg_fare=('fare_amount','mean')\n",
    "    ).sort_values('trips', ascending=False)\n",
    "    display(g3)\n"
   ]
