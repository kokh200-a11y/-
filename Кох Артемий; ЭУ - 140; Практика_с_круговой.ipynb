    "\n",
    "# Попытка загрузки через kagglehub (при отсутствии интернета/токена код просто пропустит этот шаг)\n",
    "dataset_local_path = None\n",
    "try:\n",
    "    dataset_local_path = kagglehub.dataset_download(\"yashdevladdha/uber-ride-analytics-dashboard\")\n",
    "    print(\"Dataset downloaded to:\", dataset_local_path)\n",
    "except Exception as e:\n",
    "    print(\"Не удалось загрузить через kagglehub:\", e)\n",
    "\n",
    "# Укажите имя файла CSV внутри датасета или локальный путь\n",
    "# Часто на Kaggle файлы называются чем-то вроде 'uber_ride_data.csv' — скорректируйте при необходимости.\n",
    "candidate_filenames = [\n",
    "    \"uber_data.csv\",\n",
    "    \"uber_ride_data.csv\",\n",
    "    \"uber_rides.csv\",\n",
    "    \"UberDataset.csv\",\n",
    "    \"Uber_Ride_Analytics_Dashboard.csv\"\n",
    "]\n",
    "\n",
    "csv_path = None\n",
    "if dataset_local_path:\n",
    "    import os\n",
    "    for root, _, files in os.walk(dataset_local_path):\n",
    "        for name in files:\n",
    "            if name.lower().endswith(\".csv\"):\n",
    "                if name in candidate_filenames or True:\n",
    "                    csv_path = os.path.join(root, name)\n",
    "                    break\n",
    "        if csv_path:\n",
    "            break\n",
    "\n",
    "# Если не нашли/не скачали — поставьте свой путь вручную\n",
    "if csv_path is None:\n",
    "    # Пример: csv_path = \"/path/to/your/uber_data.csv\"\n",
    "    csv_path = None\n",
    "\n",
    "print(\"CSV path:\", csv_path)\n"
   ]
