   "source": [
    "\n",
    "# Подберём существующие колонки\n",
    "has_price = 'fare_amount' in df.columns\n",
    "has_dist = 'distance_km' in df.columns or 'distance' in df.columns\n",
    "dist_col = 'distance_km' if 'distance_km' in df.columns else ('distance' if 'distance' in df.columns else None)\n",
    "has_status = 'status' in df.columns\n",
    "has_hour = 'pickup_hour' in df.columns\n",
    "has_vehicle = 'vehicle_type' in df.columns\n",
    "has_city = 'pickup_city' in df.columns\n",
    "\n",
    "# 5 фильтров через query\n",
    "filters_query = []\n",
    "if has_status:\n",
    "    filters_query.append((\"Completed only\", \"status == 'completed'\"))\n",
    "if has_price:\n",
    "    filters_query.append((\"High fare\", \"fare_amount > fare_amount.quantile(0.9)\"))\n",
    "if has_dist and dist_col:\n",
    "    filters_query.append((\"Long trips\", f\"{dist_col} > {dist_col}.quantile(0.9)\"))\n",
    "if has_hour:\n",
    "    filters_query.append((\"Peak morning\", \"pickup_hour >= 7 and pickup_hour <= 10\"))\n",
    "if has_vehicle:\n",
    "    filters_query.append((\"UberX only\", \"vehicle_type.str.lower() == 'uberx'\"))\n",
    "\n",
    "for name, q in filters_query[:5]:\n",
    "    print(f\"\\n[query] {name}: {q}\")\n",
    "    try:\n",
    "        display(df.query(q).head(5))\n",
    "    except Exception as e:\n",
    "        print(\"Ошибка в query:\", e)\n",
    "\n",
    "# 5 фильтров через where (логическая индексация)\n",
    "print(\"\\nЛогическая индексация (where):\")\n",
    "conds = []\n",
    "if has_hour:\n",
    "    conds.append((\"Late night\", df['pickup_hour'].between(22, 23)))\n",
    "if has_price:\n",
    "    conds.append((\"Low fare\", df['fare_amount'] <= df['fare_amount'].quantile(0.1)))\n",
    "if has_dist and dist_col:\n",
    "    conds.append((\"Short trips\", df[dist_col] < df[dist_col].median()))\n",
    "if has_status:\n",
    "    conds.append((\"Cancelled\", df['status'].str.lower().eq('cancelled')))\n",
    "if has_vehicle:\n",
    "    conds.append((\"XL or Premium\", df['vehicle_type'].str.contains('xl|black|premium', case=False, na=False)))\n",
    "\n",
    "for name, cond in conds[:5]:\n",
    "    print(f\"\\n[where] {name}\")\n",
    "    display(df[cond].head(5))\n"
   ]
