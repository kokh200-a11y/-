    "\n",
    "pivots = {}\n",
    "\n",
    "# 1) Кол-во поездок по месяцам и типам ТС\n",
    "if {'pickup_month','vehicle_type'} <= set(df.columns):\n",
    "    pivots['trips_by_month_vehicle'] = pd.pivot_table(df, index='pickup_month', columns='vehicle_type',\n",
    "                                                      values=df.columns[0], aggfunc='count', fill_value=0)\n",
    "    display(pivots['trips_by_month_vehicle'])\n",
    "\n",
    "# 2) Выручка по городу и часу\n",
    "if {'pickup_city','pickup_hour','fare_amount'} <= set(df.columns):\n",
    "    pivots['revenue_city_hour'] = pd.pivot_table(df, index='pickup_city', columns='pickup_hour',\n",
    "                                                 values='fare_amount', aggfunc='sum', fill_value=0)\n",
    "    display(pivots['revenue_city_hour'])\n",
    "\n",
    "# 3) Доля отмен по способу оплаты\n",
    "if {'status','payment_method'} <= set(df.columns):\n",
    "    tmp = df.assign(is_cancelled = df['status'].str.lower().eq('cancelled'))\n",
    "    pivots['cancel_rate_by_payment'] = pd.pivot_table(tmp, index='payment_method', values='is_cancelled', aggfunc='mean')\n",
    "    display(pivots['cancel_rate_by_payment'])\n",
    "\n",
    "# 4) Средний чек по типу ТС\n",
    "if {'vehicle_type','fare_amount'} <= set(df.columns):\n",
    "    pivots['avg_fare_by_vehicle'] = pd.pivot_table(df, index='vehicle_type', values='fare_amount', aggfunc='mean')\n",
    "    display(pivots['avg_fare_by_vehicle'])\n"
   ]
