    "    df.groupby('vehicle_type')['fare_amount'].mean().sort_values().plot(kind='bar')\n",
    "    plt.title('Средняя стоимость по типам ТС')\n",
    "    plt.xlabel('Тип ТС')\n",
    "    plt.ylabel('Средняя стоимость')\n",
    "    plt.grid(True, axis='y')\n",
    "    plt.show()\n",
    "\n",
    "# Matplotlib 3: количество поездок по часам\n",
    "if 'pickup_hour' in df.columns:\n",
    "    plt.figure()\n",
    "    df['pickup_hour'].value_counts().sort_index().plot(kind='line', marker='o')\n",
    "    plt.title('Количество поездок по часам')\n",
    "    plt.xlabel('Час')\n",
    "    plt.ylabel('Кол-во поездок')\n",
    "    plt.grid(True)\n",
    "    plt.show()\n",
    "\n",
    "# Seaborn 1: boxplot стоимости по типу ТС\n",
