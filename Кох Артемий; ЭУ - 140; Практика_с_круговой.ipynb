    "\n",
    "# Подсчёт пропусков\n",
    "na_counts = df.isna().sum().sort_values(ascending=False)\n",
    "display(na_counts.to_frame('missing'))\n",
    "\n",
    "# Универсальные кандидаты на временные поля и числовые поля\n",
    "time_candidates = [\n",
    "    'pickup_time','drop_time','start_time','end_time','datetime','request_time','accept_time','trip_start_timestamp','trip_end_timestamp','order_time'\n",
    "]\n",
    "numeric_candidates = ['distance_km','distance','miles','fare_amount','price','amount','revenue','surge_multiplier','rating','driver_rating','passenger_rating','duration_min','duration']\n",
    "\n",
    "# Конвертация дат, если найдём подходящие столбцы\n",
    "for c in time_candidates:\n",
    "    if c in df.columns and not np.issubdtype(df[c].dtype, np.datetime64):\n",
    "        try:\n",
    "            df[c] = pd.to_datetime(df[c], errors='coerce', dayfirst=False, infer_datetime_format=True)\n",
    "        except Exception as e:\n",
    "            print(f\"Не удалось преобразовать {c} в datetime:\", e)\n",
    "\n",
    "# Создадим удобные производные признаки, если есть pickup_time/drop_time\n",
    "if 'pickup_time' in df.columns:\n",
    "    df['pickup_date'] = df['pickup_time'].dt.date\n",
    "    df['pickup_hour'] = df['pickup_time'].dt.hour\n",
    "    df['pickup_weekday'] = df['pickup_time'].dt.day_name()\n",
    "    df['pickup_month'] = df['pickup_time'].dt.to_period('M').astype(str)\n",
    "\n",
    "if 'drop_time' in df.columns:\n",
    "    df['drop_date'] = df['drop_time'].dt.date\n",
    "    df['drop_hour'] = df['drop_time'].dt.hour\n",
    "\n",
    "# Длительность поездки (в мин.), если есть обе метки времени\n",
    "if {'pickup_time','drop_time'} <= set(df.columns):\n",
    "    df['duration_min'] = (df['drop_time'] - df['pickup_time']).dt.total_seconds() / 60\n",
    "\n",
    "# Заполним рейтинги средним по колонке, если они есть\n",
    "for col in ['driver_rating','passenger_rating','rating']:\n",
    "    if col in df.columns and df[col].isna().any():\n",
    "        df[col] = df[col].fillna(df[col].mean())\n",
    "\n",
    "# Для категориальных — заполним \"unknown\"\n",
    "for col in ['vehicle_type','payment_method','status','pickup_city','drop_city']:\n",
    "    if col in df.columns and df[col].isna().any():\n",
    "        df[col] = df[col].fillna('unknown')\n",
    "\n",
    "# Числовые поля — средним или 0 (обоснуйте в отчёте нужную стратегию)\n",
    "for col in numeric_candidates:\n",
    "    if col in df.columns and df[col].isna().any():\n",
    "        if col in ['surge_multiplier']:\n",
    "            df[col] = df[col].fillna(1.0)\n",
    "        else:\n",
    "            df[col] = df[col].fillna(df[col].median())\n",
    "\n",
    "print(\"Типы данных после преобразований:\")\n",
    "print(df.dtypes.head(20))\n"
